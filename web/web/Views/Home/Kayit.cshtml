
@{
    ViewBag.Title = "Kayit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form>
    <div class="form-group">
        <div class="form-group col-md-4">
            <label for="inputEmail4">Email</label>
            <input type="email" class="form-control" id="inputEmail4"  placeholder="Email" required>
        </div>
        <div class="form-group col-md-4">
            <label for="inputPassword4">Şifre</label>
            <input type="password" class="form-control" id="inputPassword4" placeholder="Şifre" required>
        </div>
        <div class="form-group col-md-4">
            <label for="tc">TCNO:</label>
            <input type="text" name="txtboxToFilter" id="txtboxToFilter" maxlength="11" class="form-control" placeholder="TC" required>
        </div>
   </div>
    <button type="submit" class="btn btn-primary">Kayıt Ol</button>
</form>